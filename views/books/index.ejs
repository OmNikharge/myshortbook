

<form action="/books" method="get">
<div>
<label>Title</label>
<input type="text" name="title"  value="<%= typedText.title %>">
</div>
<div>
<label>Published After</label>
<input type="date" name="publishedAfter"  value="<%= typedText.publishedAfter %>">
</div>
<div>
<label>Published Before</label>
<input type="date" name="publishedBefore"  value="<%= typedText.publishedBefore %>">
</div><br>
<button type="submit">Search</button>
</form>
<br>
<div>
  <% books.forEach(book => { %>
    <img height="150" width="100" src="<%= book.coverImagePath %> ">    
 <% }) %>   
</div>